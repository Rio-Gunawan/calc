{
    "env": {
        "browser": true,
        "commonjs": true,
        "jquery": true,
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "globals": {
        "BigNumber":true
    },
    "rules": {
        "no-unused-vars": "warn",
        "indent": [
            "warn",
            4
        ],
        "linebreak-style": [
            "warn",
            "unix"
        ],
        "semi": [
            "warn",
            "always"
        ],
        "no-console": "warn"
    }
}